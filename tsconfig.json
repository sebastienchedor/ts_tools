{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"removeComments": true,
		"target": "es6",
		"sourceMap": true,
		"outDir": ".build",
		"baseUrl": "./",
		"strict": true,
		"noImplicitReturns": true,
		"noUnusedLocals": false,
		"incremental": false
	},
	"include": ["src/**/*", "sdk/**/*"],
	"exclude": ["node_modules"]
}
